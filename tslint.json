{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name" : [true, "never-prefix"],
        "linebreak-style": false,
        "no-console": false,
        "no-debugger": false,
        "no-var-requires": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}
